{
  "TS_HOME": "/home/eduda/Projects/messaging-testsuite",
  "PWD": "${TS_HOME}/jboss-hornetq-testsuite",
  "ENV": {
    "JBOSS_HOME_1": "${TS_HOME}/server1",
    "JBOSS_HOME_2": "${TS_HOME}/server2",
    "JBOSS_HOME_3": "${TS_HOME}/server3",
    "JBOSS_HOME_4": "${TS_HOME}/server4"
  },
  "CMD": "mvn clean test -Dtest=${TEST} -DfailIfNoTests=false ${MVN_PROFILE} ${JDBC_ARGS}",
  "OUT": "${PWD}/log",
  "JDBC_ARGS": "",
  "profiles": {
    "default": "eap7",
    "eap7": {
      "MVN_PROFILE": "-Deap=7x"
    },
    "eap6": {
      "MVN_PROFILE": "-Deap=6x"
    },
    "jdbc": {
      "JDBC_ARGS": "-Dprepare.param.DATABASE=oracle12c -Dprepare.param.JDBC_STORE=true"
    },
    "jdbc-local": {
      "JDBC_ARGS": "-Dprepare.param.DATABASE=oracle12c -Dprepare.param.JDBC_STORE=true \"-Dprepare.param.DATABASE_JDBC_URL=jdbc:oracle:thin:@localhost:1521:xe\" -Dprepare.param.DATABASE_USER_NAME=system -Dprepare.param.DATABASE_PASSWORD=oracle -Dprepare.param.DATABASE_JDBC_CLASS=oracle.jdbc.driver.OracleDriver -Dprepare.param.DATABASE_JDBC_CLASS_XA=oracle.jdbc.xa.client.OracleXADataSource"
    }
  }
}
